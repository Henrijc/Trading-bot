{
    "bot_name": "CryptoCoachBot",
    "max_open_trades": 3,
    "stake_currency": "ZAR",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "ZAR",
    "timeframe": "1h",
    "dry_run": true,
    "dry_run_wallet": 154273.71,
    "cancel_open_orders_on_exit": false,

    "exchange": {
        "name": "luno",
        "key": "YOUR_LUNO_API_KEY",
        "secret": "YOUR_LUNO_SECRET_KEY",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": [
            "BTC/ZAR",
            "ETH/ZAR", 
            "XRP/ZAR"
        ],
        "pair_blacklist": []
    },

    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },

    "exit_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1
    },

    "order_types": {
        "entry": "limit",
        "exit": "limit",
        "emergency_exit": "market",
        "force_exit": "market",
        "force_entry": "market",
        "stoploss": "market",
        "stoploss_on_exchange": false,
        "stoploss_on_exchange_interval": 60
    },

    "order_time_in_force": {
        "entry": "gtc",
        "exit": "gtc"
    },

    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],

    "edge": {
        "enabled": false,
        "process_throttle_secs": 3600,
        "calculate_since_number_of_days": 14,
        "allowed_risk": 0.01,
        "stoploss_range_min": -0.01,
        "stoploss_range_max": -0.1,
        "stoploss_range_step": -0.01,
        "minimum_winrate": 0.60,
        "minimum_expectancy": 0.20,
        "min_trade_number": 10,
        "max_trade_duration_minute": 1440,
        "remove_pumps": false
    },
    
    "freqai": {
        "enabled": true,
        "purge_old_models": true,
        "train_period_days": 30,
        "backtest_period_days": 7,
        "live_retrain_hours": 24,
        "expiration_hours": 48,
        "identifier": "LunoFreqAI",
        "feature_parameters": {
            "include_timeframes": ["5m", "15m", "4h"],
            "include_corr_pairlist": [
                "BTC/ZAR",
                "ETH/ZAR"
            ],
            "label_period_candles": 24,
            "include_shifted_candles": 2,
            "weight_factor": 0.9,
            "principal_component_analysis": false,
            "use_SVM_to_remove_outliers": true,
            "DI_threshold": 0.9,
            "noise_level": 0.01
        },
        "data_split_parameters": {
            "test_size": 0.33,
            "shuffle": false
        },
        "model_training_parameters": {
            "n_estimators": 100,
            "random_state": 42,
            "max_depth": 10,
            "min_samples_split": 20
        }
    },

    "api_server": {
        "enabled": true,
        "listen_ip_address": "0.0.0.0",
        "listen_port": 8082,
        "verbosity": "error",
        "enable_openapi": true,
        "jwt_secret_key": "your_jwt_secret_here",
        "CORS_origins": ["*"],
        "username": "freqtrader",
        "password": "freqtrader_password"
    },

    "initial_state": "running",

    "force_entry_enable": false,

    "internals": {
        "process_throttle_secs": 5
    },

    "strategy": "LunoFreqAIStrategy",
    "strategy_path": "user_data/strategies/",
    
    "minimal_roi": {
        "60": 0.01,
        "30": 0.02,
        "0": 0.04
    },

    "stoploss": -0.04,

    "trailing_stop": false,
    "trailing_stop_positive": 0.01,
    "trailing_stop_positive_offset": 0.0,
    "trailing_only_offset_is_reached": false,

    "datadir": "user_data/data",
    "user_data_dir": "user_data",

    "logfile": "logs/freqtrade.log",

    "db_url": "sqlite:///tradesv3.sqlite",

    "add_config_files": []
}