# AI Crypto Trading Coach - Frontend Environment Configuration
# Copy this file to .env and place in the frontend directory

# Backend API URL (Pre-configured for Docker)
REACT_APP_BACKEND_URL=http://localhost:8001/api

# WebSocket Configuration
WDS_SOCKET_PORT=443

# Development Settings
CHOKIDAR_USEPOLLING=true
WATCHPACK_POLLING=true
FAST_REFRESH=true

# Build Configuration
GENERATE_SOURCEMAP=true
REACT_APP_VERSION=1.0.0